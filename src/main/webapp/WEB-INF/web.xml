<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee          http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>UserContents</servlet-name>
        <servlet-class>kic.UserContents</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubmitEntity</servlet-name>
        <servlet-class>kic.SubmitEntity</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReceiveEntity</servlet-name>
        <servlet-class>kic.ReceiveEntity</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubmitKeyword</servlet-name>
        <servlet-class>kic.SubmitKeyword</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReceiveKeyword</servlet-name>
        <servlet-class>kic.ReceiveKeyword</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StorageTester</servlet-name>
        <servlet-class>kic.StorageTester</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>kic.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminPage</servlet-name>
        <servlet-class>kic.adminPage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>clientPage</servlet-name>
        <servlet-class>kic.clientPage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReceiveAdmin</servlet-name>
        <servlet-class>kic.ReceiveAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReceiveEditUser</servlet-name>
        <servlet-class>kic.ReceiveEditUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Translation</servlet-name>
        <servlet-class>kic.Translation</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserContents</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubmitEntity</servlet-name>
        <url-pattern>/submit_entity</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReceiveEntity</servlet-name>
        <url-pattern>/receive_entity</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubmitKeyword</servlet-name>
        <url-pattern>/submit_keyword</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReceiveKeyword</servlet-name>
        <url-pattern>/receive_keyword</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StorageTester</servlet-name>
        <url-pattern>/storage_tester</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminPage</servlet-name>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>clientPage</servlet-name>
        <url-pattern>/client</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReceiveAdmin</servlet-name>
        <url-pattern>/addNewUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReceiveEditUser</servlet-name>
        <url-pattern>/edit_user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Translation</servlet-name>
        <url-pattern>/translation</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
